<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="EyeBoard.Logic.Models"
                   assembly="EyeBoard.Logic">
  <joined-subclass name="Video" extends="AdminModel" table="Videos">
    <key column="Id" />
    
    <property name="Title" />
    <property name="Url"/>
    <property name="Ordering" />
    <property name="PublishUp"/>
    <property name="PublishDown"/>
    
  </joined-subclass>

</hibernate-mapping>